{
  "name": "yumio",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:all": "npm run build:libs:all && npm run build:apps:all",
    "build:libs:all": "nest build common && nest build modules",
    "build:libs:all:watch": "nodemon",
    "build:apps:all": "npm run prebuild && npm run build:libs:all && npm run build:apps:yumio",
    "build:apps:yumio": "nest build yumio",
    "build:apps:admin": "nest build admin",
    "build:apps:webApp": "vite --config apps/webApp/src/vite.config.ts build",
    "build:apps:webApp:watch": "nodemon --watch apps/webApp --ext tsx,json --exec \"vite --config apps/webApp/src/vite.config.ts build\"",
    "build:apps:yumio:watch": "nodemon --watch services/ --ext ts,json --exec \"npm run build:apps:yumio\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:apps:dev:yumio": "nest start yumio --watch",
    "start:apps:dev:admin": "nest start admin --watch",
    "start:apps:dev:webApp": "vite --config apps/webApp/src/vite.config.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "npm run test:unit:yumio",
    "test:unit:yumio": "jest --config ./apps/yumio/test/jest-unit.json --passWithNoTests",
    "test:unit:yumio:watch": "jest --config ./apps/yumio/test/jest-unit.json  --watch",
    "test:functional:yumio": "jest --config ./apps/yumio/test/jest-functional.json  --detectOpenHandles --runInBand --forceExit --passWithNoTests",
    "test:functional:yumio:watch": "jest --config ./apps/yumio/test/jest-functional.json --detectOpenHandles --runInBand --forceExit --watch"
  },
